{
  "name": "sveltekit-demo",
  "version": "1.0.2",
  "scripts": {
    "dev:only": "svelte-kit dev",
    "build:only": "svelte-kit build",
    "preview": "svelte-kit preview",
    "tailwind:watch": "cross-env TAILWIND_MODE=watch cross-env NODE_ENV=development postcss src/styles/tailwind.css -o src/styles/tailwind-output.css -w",
    "tailwind:build": "cross-env TAILWIND_MODE=build cross-env NODE_ENV=production  postcss src/styles/tailwind.css -o src/styles/tailwind-output.css",
    "dev": "concurrently \"yarn run dev:only\" \"yarn run tailwind:watch\"",
    "build": "yarn run tailwind:build && yarn run build:only",
    "lint": "prettier --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --write --plugin-search-dir=. .",
		"test": "vitest",
		"test:run": "vitest --run",
		"test:ui": "vitest --ui",
		"test:ci": "vitest --run --coverage"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@sveltejs/adapter-netlify": "^1.0.0-next.31",
    "@sveltejs/adapter-vercel": "^1.0.0-next.47",
    "@sveltejs/kit": "^1.0.0-next.315",
    "@testing-library/svelte": "^3.1.0",
    "@types/google.maps": "^3.47.2",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "autoprefixer": "^10.4.2",
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "cssnano": "^5.0.17",
    "d3": "^7.0.1",
    "eslint": "^8.8.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.4.0",
    "jsdom": "^19.0.0",
    "postcss": "^8.4.6",
    "postcss-cli": "^9.1.0",
    "postcss-load-config": "^3.1.1",
    "prettier": "~2.5.1",
    "prettier-plugin-svelte": "^2.6.0",
    "rimraf": "^3.0.2",
    "simple-svelte-autocomplete": "^2.2.4",
    "svelte": "^3.47.0",
    "svelte-preprocess": "^4.10.6",
		"tailwindcss": "^3.0.24",
    "tslib": "^2.3.1",
		"typescript": "^4.6.3",
    "vitest": "^0.8.4",
		"vitest-svelte-kit": "^0.0.6"
  },
  "type": "module",
  "description": "Svelte Kit Experiments in an kitchensink app",
  "main": "svelte.config.js",
  "dependencies": {
    "@lukeed/uuid": "^2.0.0",
    "@tailwindcss/forms": "^0.4.0",
    "cities.json": "^1.1.5",
    "cookie": "^0.4.2",
    "countries-list": "^2.6.1",
    "firebase": "^9.6.6",
    "graphql": "^16.3.0",
    "graphql-request": "^4.0.0",
    "stensvelte_hydrate": "0.0.2"
  },
  "author": "Tarun Kumar Sukhu",
  "license": "MIT"
}