		"@types/google.maps": "^3.47.2",
		"autoprefixer": "^10.4.7",
		"concurrently": "^7.0.0",
		"cross-env": "^7.0.3",
		"cssnano": "^5.0.17",
		"d3": "^7.0.1",
		"jsdom": "^19.0.0",
		"postcss": "^8.4.14",
		"postcss-load-config": "^4.0.1",
		"postcss-cli": "^10.0.0",
		"rimraf": "^3.0.2",
		"simple-svelte-autocomplete": "^2.4.0",
		"tailwindcss": "^3.1.8",
		"vitest": "^0.23.2",
